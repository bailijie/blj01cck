<view><view class="user"><view class="left"><image src="{{userAvatar?userAvatar:'/static/logo.jpg'}}" data-event-opts="{{[['tap',[['toSetting',['$event']]]]]}}" bindtap="__e"></image></view><view class="right"><view data-event-opts="{{[['tap',[['toSetting',['$event']]]]]}}" class="username" bindtap="__e">{{userName}}</view><view class="rightxt"><text>{{"真品"+zl[1]}}</text><text>{{"赝品"+zl[2]}}</text><text>{{"存疑"+zl[3]}}</text></view></view></view><view><view class="topTabBar"><block wx:for="{{orderType}}" wx:for-item="grid" wx:for-index="tbIndex" wx:key="tbIndex"><view data-event-opts="{{[['tap',[['showType',[tbIndex]]]]]}}" class="grid" bindtap="__e"><view class="{{['text',tbIndex==tabbarIndex?'on':'']}}">{{grid}}</view></view></block></view><view class="order-list"><view class="list"><block wx:if="{{list.length==0}}"><view class="onorder"><view class="text">没有相关订单</view></view></block><block wx:for="{{list}}" wx:for-item="row" wx:for-index="index" wx:key="index"><uni-swipe-action class="row" vue-id="{{'1-'+index}}" disabled="{{!(row.isReal==null&&row.payStatus==1)}}" options="{{options}}" data-event-opts="{{[['^click',[['delClick',['$0'],[[['list','',index]]]]]]]}}" bind:click="__e" bind:__l="__l" vue-slots="{{['default']}}"><block wx:if="{{row.isReal==null&&row.payStatus==2&&row.appraisalStatus==0}}"><view class="type">鉴定中</view></block><block wx:if="{{row.isReal==null&&row.payStatus==1}}"><view class="type">未发布</view></block><block wx:if="{{row.isReal==1}}"><view class="{{['type',(row.isReal==1)?'jin':'']}}">真品</view></block><block wx:if="{{row.isReal==2}}"><view class="type">赝品</view></block><block wx:if="{{row.isReal==3}}"><view class="{{['type',(row.isReal==3)?'hui':'']}}">存疑</view></block><view class="order-info"><view class="left"><image class="{{[(row.isReal==3)?'hui':'']}}" src="{{row.opics[0].pic}}"></image></view><view class="right"><view class="name">{{row.productName}}</view><view class="name">{{row.productIntroduce}}</view><view class="time">{{row.createTime}}</view></view><view><block wx:if="{{row.isReal==null&&row.payStatus==2&&row.appraisalStatus==0}}"><view class="progress">鉴定中</view></block><block wx:if="{{row.isReal==null&&row.payStatus==1}}"><view data-event-opts="{{[['tap',[['pay',['$0'],[[['list','',index]]]]]]]}}" class="progress" bindtap="__e">去支付</view></block><block wx:if="{{row.isReal}}"><view class="{{['progress',(row.isReal==2||row.isReal==3)?'action':'']}}">已完成</view></block><block wx:if="{{row.isReal}}"><view data-event-opts="{{[['tap',[['details',['$0'],[[['list','',index]]]]]]]}}" class="{{['progress',(row.isReal==2||row.isReal==3)?'action':'']}}" bindtap="__e">鉴定结果</view></block></view></view></uni-swipe-action></block></view></view></view></view>