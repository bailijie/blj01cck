.choose-zhuanjia {
	width: 100%;
	height: 100%;
	position: fixed;
	top: 0;
	left: 0;
	z-index: 9999;
	background: rgba(0, 0, 0, 0.5);
}
.zhuanjia-list {
	width: 100%;
	height: 600rpx;
	background: #ffffff;
	position: absolute;
	bottom: 0;
}
.choice {
	text-align: center;
	height: 130rpx;
	line-height: 130rpx;
	border-bottom: #e6e6e6 solid 1rpx;
}
.choice text {
	font-size: 36rpx;
}
.choice .quX {
	position: absolute;
	right: 0;
	margin-right: 30rpx;
}
.actions {
	text-align: left;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
	height: 160rpx;
	margin-left: 30rpx;
	border-bottom: #e6e6e6 solid 1rpx;
}
.actions image {
	-webkit-border-radius: 50%;
	        border-radius: 50%;
	width: 108rpx;
	height: 108rpx;
}
.actions .Zjname {
	font-size: 28rpx;
}
.actions .Zjshan {
	font-size: 26rpx;
	color: #999999;
}
