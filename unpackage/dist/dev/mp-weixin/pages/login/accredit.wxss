
.inner {
	height: 100vh;
	background: #fff;
	padding: 32rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
}
.logo {
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	    -ms-flex-pack: center;
	        justify-content: center;
	-webkit-border-radius: 50%;
	        border-radius: 50%;
	text-align: center;
	margin: 30rpx;
}
.logo image{
	width: 180rpx;
	height: 180rpx;
	-webkit-border-radius: 50%;
	        border-radius: 50%;
}
.text {
	text-align: center;
}
.im {
	margin-top: 100rpx;
}
.im button {
	display: block;
	height: 80rpx;
	-webkit-border-radius: 4px;
	        border-radius: 4px;
	background-color: #cfb789;
	color: rgba(255, 255, 255, 1);
	font-size: 28rpx;
	text-align: center;
	line-height: 80rpx;
}
button[type='primary'] {
	color: #ffffff;
	background-color: #cfb789 !important;
}

